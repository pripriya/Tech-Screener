<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.geval6.techscreener">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Source.Activities.LaunchActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Source.Activities.SetupActivity"
            android:noHistory="true"
            />

        <activity
            android:name=".Source.Activities.AdminActivity"
            android:label="Clients"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".Source.Activities.ClientActivity"
            android:label="Client" />
        <activity
            android:name=".Source.Activities.AssessorActivity"
            android:label="Assessor" />
    </application>

</manifest>